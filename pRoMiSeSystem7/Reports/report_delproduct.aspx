				AdditionalHeaderText = ReceiptHeaderData.Rows(0)("DocumentTypeHeader")
			End If
		End If
		
		Dim TextClass As String = "text"

		Dim i As Integer
		Dim FullText As String
		Dim RCNumber As Boolean

		For i = 0 To dtTable.Rows.Count - 1
		
			HText  = ""
			If PropertyInfo.Rows(0)("FrontSystemType") = 1 Then
				If Not IsDBNull(dtTable.Rows(i)("DocumentTypeHeader")) Then
					HText = dtTable.Rows(i)("DocumentTypeHeader")
				End If
			Else
				HText = AdditionalHeaderText
			End If
			RCNumber = False
			If IsDBNull(dtTable.Rows(i)("ReceiptID")) Or IsDBNull(dtTable.Rows(i)("ReceiptMonth")) Or IsDBNull(dtTable.Rows(i)("ReceiptYear")) Then
				RText = "-"
			Else If dtTable.Rows(i)("ReceiptID") = 0 Then
				RText = "Manual Close (# " + dtTable.Rows(i)("ComputerID").ToString + "/" + dtTable.Rows(i)("TransactionID").ToString + ")"
			Else
				RText = FormatDocNumber.getReceiptHeader(HText,dtTable.Rows(i)("ReceiptYear"),dtTable.Rows(i)("ReceiptMonth"),dtTable.Rows(i)("ReceiptID"))
				RCNumber = True
			End If
			
			outputString += "<td align=""left"" class=""" + TextClass + """>" + (i+1).ToString + "</td>"
			If Not IsDBNull(dtTable.Rows(i)("DelStaffName")) Then
				outputString += "<td align=""left"" class=""" + TextClass + """>" + dtTable.Rows(i)("DelStaffName") + "</td>"
			Else
				outputString += "<td align=""left"" class=""" + TextClass + """>" + "-" + "</td>"
			End If
			
			If RCNumber = True Then
				outputString += "<td align=""left"" class=""text""><a href=""JavaScript: newWindow = window.open( '../Reports/BillDetails.aspx?ComputerID=" + dtTable.Rows(i)("ComputerID").ToString + "&TransactionID=" + dtTable.Rows(i)("TransactionID").ToString + "', '', 'width=750,height=600,toolbar=0,location=0,directories=0,status=0,menuBar=0,scrollBars=1,resizable=1' ); newWindow.focus()"">" & RText & "</a></td>"
			Else
				outputString += "<td align=""left"" class=""text"">" & RText & "</td>"
			End If
			
			outputString += "<td align=""right"" class=""" + TextClass + """>" + DateTimeUtil.FormatDateTime(dtTable.Rows(i)("ChangeDateTime"), "DateAndTime") + "</td>"

			
			If Not IsDBNull(dtTable.Rows(i)("ProductName")) Then
				outputString += "<td align=""left"" class=""" + TextClass + """>" + dtTable.Rows(i)("ProductName") + "</td>"
			Else
				outputString += "<td align=""left"" class=""" + TextClass + """>" + "-" + "</td>"
			End If
			
			If Not IsDBNull(dtTable.Rows(i)("ProductPrice")) Then
				outputString += "<td align=""right"" class=""" + TextClass + """>" + Format(dtTable.Rows(i)("ProductPrice"), "##,##0.00") + "</td>"
			Else
				outputString += "<td align=""right"" class=""" + TextClass + """>" + "-" + "</td>"
			End If

			outputString += "</tr>"
		Next

		ResultText.InnerHtml = outputString
	End If
	
End Sub
     
Sub Page_UnLoad()
	objCnn.Close()
End Sub

</script>
